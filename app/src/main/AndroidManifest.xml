<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lollipop.kotlinnotes">

    <application
        android:name="com.lollipop.application.LollipopApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.lollipop.activity.CoroutineActivity" />
        <activity android:name="com.lollipop.dragview.DragViewActivity" />
        <activity android:name="com.lollipop.jetpack.room.RoomActivity" />
        <activity android:name="com.lollipop.activity.SecondActivity" />
        <activity android:name="com.lollipop.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.shortcuts"
                android:resource="@xml/shortcuts" />
        </activity>
    </application>

</manifest>